define	hidms : tpcalib
file    dev/hid/hidms.c			hidms

define	hidkbd
file	dev/hid/hidkbd.c		hidkbd

file	dev/hid/hidkbdmap.c		ukbd | btkbd | ikbd | linux_keymap

