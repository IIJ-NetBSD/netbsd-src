require allow_python_tests
require {!is_remote host}

save_vars { env(PYTHONHOME) } {
    setenv PYTHONHOME foo
    clean_restart
}

gdb_test "python print (1)" \
    "Python not initialized"

gdb_test_multiple "quit" "" {
    eof {
	pass $gdb_test_name
    }
}
